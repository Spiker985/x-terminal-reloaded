{
  "extends": [
    "config:base"
  ],
  "devDependencies": {
    "automerge": true,
    "commitMessageTopic": "devDependency {{depName}}"
  },
  "packageRules": [
    {
      "matchPackagePatterns": "^xterm",
      "groupName": "xterm"
    },
    {
      "matchPackagePatterns": "^eslint",
      "groupName": "eslint"
    },
    {
      "matchPackagePatterns": "^stylelint",
      "groupName": "stylelint"
    }
  ],
  "rangeStrategy": "bump"
}
